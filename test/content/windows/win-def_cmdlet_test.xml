    <definitions>
        <definition id="oval:org.mitre.oval.test:def:378" version="1" class="miscellaneous">
            <metadata>
                <title>Evaluate to true if the win-def:cmdlet_test is properly supported</title>
                <description>This definition is intended to evalutate to true if the interpreter properly supports the win-def:cmdlet_test.</description>
            </metadata>
            <notes>
                <note>It is important to note that the values used in this test definition are specific to a particular machine, and may not necessarily apply to your system. As a result, in order to have the definition return a result of 'true', you must either change these values to the ones found on your particular system, or you must configure your system to use these values.</note>
            </notes>
            <criteria operator="AND">
                <criterion comment="Test that a cmdlet_object is supported." test_ref="oval:org.mitre.oval.test:tst:944"/>
            </criteria>
        </definition>
    </definitions>
    <tests>
        <cmdlet_test id="oval:org.mitre.oval.test:tst:944" version="1" comment="Test that a cmdlet_object with a security_principle is supported." check_existence="at_least_one_exists" check="at least one" xmlns="http://oval.mitre.org/XMLSchema/oval-definitions-5#windows">
            <object object_ref="oval:org.mitre.oval.test:obj:125"/>
            <state state_ref="oval:org.mitre.oval.test:ste:125"/>
        </cmdlet_test>
    </tests>
    <objects>
        <cmdlet_object id="oval:org.mitre.oval.test:obj:125" version="1" comment="A cmdlet_object that retrieves a list of processes." xmlns="http://oval.mitre.org/XMLSchema/oval-definitions-5#windows">
            <module_name>BitsTransfer</module_name>
            <module_id>{8FA5064B-8479-4c5c-86EA-0D311FE48875}</module_id>
            <module_version datatype="version">1.0.0.0</module_version>
            <verb>Get</verb>
            <noun>Module</noun>
            <parameters>
                <oval-def:field name="-listavailable"/>
                <oval-def:field name="-all"/>
                <oval-def:field name="-name">PSDiagnostics</oval-def:field>
            </parameters>
            <select>
                <oval-def:field name="moduletype"/>
                <oval-def:field name="name"/>
            </select>
        </cmdlet_object>
    </objects>
    <states>
        <cmdlet_state id="oval:org.mitre.oval.test:ste:125" version="1" comment="A cmdlet_state." xmlns="http://oval.mitre.org/XMLSchema/oval-definitions-5#windows">
