                <oval-def:field name="-listavailable"></oval-def:field>
                <oval-def:field name="-all"></oval-def:field>
                <oval-def:field name="-name">PSDiagnostics</oval-def:field>
            </parameters>
            <select>
                <oval-def:field name="moduletype"></oval-def:field>
                <oval-def:field name="name"></oval-def:field>
            </select>
            <value datatype="record" entity_check="at least one">
                <oval-def:field name="moduletype" datatype="string">Manifest</oval-def:field>
                <oval-def:field name="name" datatype="string">PSDiagnostics</oval-def:field>
            </value>
        </cmdlet_state>
